<!DOCTYPE html>
<html xmlns:ng="http://angularjs.org">
<head>
	<title>{{ title }}</title>
</head>
<body>
	{{#user}}
		<a href="/signout">Sign Out</a>
	{{/user}}
	{{^user}}
		<a href="/signup">Signup</a>
		<a href="/signin">Signin</a>
	{{/user}}

	<section ng-view>
	</section>

	<script type="text/javascript">
		{{#user}}
			window.user = {{user}}
		{{/user}}
		{{^user}}
			window.user = 'null'
		{{/user}}
	</script>

<script type="text/javascript" src="/lib/angular/angular.js" />
<script type="text/javascript" src="/lib/angular-route/angular-route.js" />

<script type="text/javascript" src="/example/example.client.module.js" />

<script type="text/javascript" src="/example/controllers/example.client.controller.js" />

<script type="text/javascript" src="/example/config/example.client.routes.js" />

<script type="text/javascript" src="/users/users.client.module.js" />

<script type="text/javascript" src="/users/services/authentication.client.service.js" />

<script type="text/javascript" src="/application.js" />

</body>
</html>
